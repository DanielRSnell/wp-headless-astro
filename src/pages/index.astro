---
export const prerender = true;
import '../styles/remote-tw.css'
import { CleanMarkup } from '../libs/clean.mjs'
const { pathname } = Astro.url;
// Routing 
const route = await fetch(import.meta.env.CORE_URL + pathname);
const payload = await route.text()
const Document = payload.split('https://domartisan.com').join('');



---

<Fragment slot="head">
    <meta http-equiv="Content-Type" content="text/html;charset=UTF-8">
</Fragment>

<Fragment set:html={Document} />
