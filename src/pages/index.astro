---
export const prerender = true;
import '../styles/remote-tw.css'
import '../styles/remote-bundle.css'
import { CleanMarkup } from '../libs/clean.mjs'
const { pathname } = Astro.url;
// Routing 
const route = await fetch(import.meta.env.CORE_URL + pathname);
const payload = await route.text();

const Document = await CleanMarkup(payload);


---

<html lang="en"><head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

<script is:inline src="/wp-content/uploads/scripts/htmx.js" ></script>
<script defer src="/wp-content/uploads/scripts/htmx/morph.js" ></script> 
<script is:inline defer src="https://unpkg.com/htmx.org/dist/ext/preload.js"></script>
<script defer src="/wp-content/uploads/scripts/anime.min.js" ></script>
<script defer type="module" src="/wp-content/uploads/assets/alpine.js" ></script>
<script defer>

    htmx.config.globalViewTransitions = true;
</script>
</head>
<Fragment set:html={Document} />
